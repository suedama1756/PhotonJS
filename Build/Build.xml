<project name="photon" default="build">
    <target name="build">
        <property name="MSBuild" value="C:\Windows\Microsoft.NET\Framework\v4.0.30319\MSBuild.exe"/>
        <property name="MSBuildProject" file="Build.msbuild" value="Build.msbuild"/>

        <exec command="${MSBuild} ${MSBuildProject}" logoutput="true"/>
    </target>
    <target name="build-all">
        <property name="MSBuild" value="C:\Windows\Microsoft.NET\Framework\v4.0.30319\MSBuild.exe"/>
        <property name="MSBuildProject" file="Build.msbuild" value="Build.msbuild"/>

        <exec command="${MSBuild} ${MSBuildProject} /p:IncludeSlickGrid=true /p:IncludeJQueryUI=true /p:IncludeValidation=true"
              logoutput="true"/>
    </target>
</project>